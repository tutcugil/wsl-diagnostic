{
  "AllowedHosts": "*",
  "Config": {
    "Path": "/app/appsettings.json"
  },
  "App": {
    "Project": "wsltest",
    "Name": "wsltest",
    "Version": "0.0.1"
  },
  "Logging": {
    "Target": "file",
    "Enabled": 1,
    "FilePath": "/var/log/wsltest/{App}/{Type}/{yyyy}/{yyyy}{MM}/{yyyy}{MM}{dd}/{FileName}_{UserId}.txt",
    "FormatType": 0,
    "Format": "{TraceTime}|{Version}|{Type}|{SourceIP}|{SourcePort}|{RequestId}|{BatchId}|{SequenceId}|USR:{UserId}|{Category}|{Message}|{StackTrace}|{Payload}",
    "MaxFileCount": 10,
    "MaxFileSize": 100
  }
}